    // "rust-analyzer.inlayHints.closureReturnTypeHints": true,
    // "rust-analyzer.inlayHints.reborrowHints": true,
    "clangd.inlayHints.enable": true,

    "clangd.disableSnippetCompletion": true,
    // "clangd.disableCompletion": true,

    "intelephense.licenceKey":"asdasdasdasdasd",

    "languageserver": {
